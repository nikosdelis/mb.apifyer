<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <PackageId>NikosDelis.OpenSource.MbApifyer</PackageId>
    <Version>1.0.0</Version>
    <Authors>Nikos Delis</Authors>
    <Product>Mb.APIfyer</Product>
    <Description>A Class Library used to wrap the request-response behaviour of message brokers to make them feel like APIs.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/nikosdelis/mb.apifyer</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="mqttnet" Version="4.3.3.952" />
    <None Include="$(SolutionDir)README.md" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>
